{"ast":null,"code":"import { jsxs as _jsxs } from \"react/jsx-runtime\";\nimport { jsx as _jsx } from \"react/jsx-runtime\";\nimport React, { useState } from 'react';\nimport { CursorPagination } from '@zendeskgarden/react-pagination';\nexport default function Pagination({\n  allJobs\n}) {\n  const {\n    0: cursor,\n    1: setCursor\n  } = useState(0);\n  const pages = allJobs;\n\n  const onFirst = () => setCursor(0);\n\n  const onLast = () => setCursor(pages.length - 1);\n\n  const onNext = () => {\n    if (cursor < pages.length - 1) {\n      setCursor(cursor + 1);\n    }\n  };\n\n  const onPrevious = () => {\n    if (cursor > 0) {\n      setCursor(cursor - 1);\n    }\n  };\n\n  return /*#__PURE__*/_jsxs(CursorPagination, {\n    \"aria-label\": \"Cursor pagination\",\n    children: [/*#__PURE__*/_jsx(CursorPagination.First, {\n      onClick: onFirst,\n      disabled: cursor === 0,\n      children: \"First\"\n    }), /*#__PURE__*/_jsx(CursorPagination.Previous, {\n      onClick: onPrevious,\n      disabled: cursor === 0,\n      children: \"Previous\"\n    }), /*#__PURE__*/_jsx(CursorPagination.Next, {\n      onClick: onNext,\n      disabled: cursor === pages.length - 1,\n      children: \"Next\"\n    }), /*#__PURE__*/_jsx(CursorPagination.Last, {\n      onClick: onLast,\n      disabled: cursor === pages.length - 1,\n      children: \"Last\"\n    })]\n  });\n}\n;","map":null,"metadata":{},"sourceType":"module"}